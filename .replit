modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_openai:1.0.0", "zendesk:1.0.0", "python_database:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "glibcLocales", "libiconv", "rustc"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "n1ago-server"

[[workflows.workflow]]
name = "n1ago-server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 39583
externalPort = 3000

[userenv]

[userenv.development]
